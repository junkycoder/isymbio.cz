{
    "source": "./markdown",
    "destination": "../web",
    "clean": false,
    "plugins": {
        "metalsmith-markdown": true,
        "metalsmith-permalinks": {
            "pattern": ":type/:name"
        },
        "metalsmith-layouts": {
            "default": "article.hbs",
            "engine": "handlebars"
        }
    }
}
